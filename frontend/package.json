{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "build:check": "npm run type-check && vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "type-check": "tsc -b --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:integration": "playwright test",
    "test:integration:ui": "playwright test --ui",
    "test:integration:debug": "playwright test --debug",
    "test:integration:headed": "playwright test --headed",
    "test:integration:report": "playwright show-report",
    "test:e2e": "playwright test src/test/integration/e2e",
    "test:api": "vitest --run src/test/integration/api",
    "test:responsive": "playwright test src/test/integration/responsive",
    "test:performance": "playwright test src/test/integration/performance",
    "test:accessibility": "playwright test src/test/integration/accessibility",
    "test:history": "playwright test src/test/integration/history",
    "test:history:integration": "playwright test src/test/integration/history/history-integration.test.ts",
    "test:history:api": "vitest --run src/test/integration/history/history-api-integration.test.ts",
    "test:history:performance": "playwright test src/test/integration/history/history-performance.test.ts",
    "test:history:comprehensive": "npx ts-node src/test/integration/history/run-history-tests.ts",
    "test:history:parallel": "npx ts-node src/test/integration/history/run-history-tests.ts --parallel",
    "test:integration:comprehensive": "npx ts-node src/test/integration/run-integration-tests.ts",
    "test:integration:parallel": "npx ts-node src/test/integration/run-integration-tests.ts --parallel",
    "test:all": "npm run test && npm run test:integration",
    "test:ci": "npm run test:all && npm run test:integration:comprehensive",
    "preview": "vite preview",
    "preview:prod": "vite preview --mode production",
    "clean": "rm -rf dist",
    "prepare": "husky",
    "prebuild": "npm run clean && npm run lint && npm run format:check"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.3",
    "@types/react-window": "^1.8.8",
    "antd": "^5.27.5",
    "axios": "^1.12.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-window": "^2.2.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.56.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "axios-mock-adapter": "^2.1.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lighthouse": "^12.8.2",
    "lint-staged": "^16.2.4",
    "prettier": "^3.6.2",
    "puppeteer": "^24.25.0",
    "terser": "^5.44.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  }
}
